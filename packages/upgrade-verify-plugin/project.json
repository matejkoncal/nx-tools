{
	"name": "upgrade-verify-plugin",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/upgrade-verify-plugin/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/js:swc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/upgrade-verify-plugin",
				"main": "packages/upgrade-verify-plugin/src/index.ts",
				"tsConfig": "packages/upgrade-verify-plugin/tsconfig.lib.json",
				"assets": [
					"packages/upgrade-verify-plugin/*.md",
					{
						"input": "./packages/upgrade-verify-plugin/src",
						"glob": "**/!(*.ts)",
						"output": "./src"
					},
					{
						"input": "./packages/upgrade-verify-plugin/src",
						"glob": "**/*.d.ts",
						"output": "./src"
					},
					{
						"input": "./packages/upgrade-verify-plugin",
						"glob": "generators.json",
						"output": "."
					},
					{
						"input": "./packages/upgrade-verify-plugin",
						"glob": "executors.json",
						"output": "."
					}
				]
			}
		},
		"publish": {
			"command": "node tools/scripts/publish.mjs upgrade-verify-plugin {args.ver} {args.tag}",
			"dependsOn": ["build"]
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"packages/upgrade-verify-plugin/**/*.ts",
					"packages/upgrade-verify-plugin/package.json"
				]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "packages/upgrade-verify-plugin/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		}
	},
	"tags": []
}
